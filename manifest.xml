<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
            xsi:type="MailApp">

  <Id>fb5ac1f2-99a9-43d4-8b47-123456789abc</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>STEGA</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="STEGA Add-in"/>
  <Description DefaultValue="Secure your emails with STEGA verification."/>
  <IconUrl DefaultValue="https://stega-project.github.io/stega-outlook-addin/icon.png"/>
  <HighResolutionIconUrl DefaultValue="https://stega-project.github.io/stega-outlook-addin/icon.png"/>

  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.1">
      <Set Name="Mailbox"/>
    </Sets>
  </Requirements>

  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://stega-project.github.io/stega-outlook-addin/taskpane.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadItem</Permissions>
</OfficeApp>
